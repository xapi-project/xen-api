(rule
  (targets COPYING)
  (deps
    ../../LICENSE
  )
  (action (copy %{deps} %{targets}))
)

(rule
  (targets README)
  (deps
    ../README.dist
  )
  (action (copy %{deps} %{targets}))
)

(alias
  (name generate)
  (deps
    COPYING
    README
    (source_tree .)
  )
)

(data_only_dirs src include)
