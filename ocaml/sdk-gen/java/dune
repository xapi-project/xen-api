(executable
  (modes byte exe)
  (name main)
  (libraries
    astring
    CommonFunctions
    dune-build-info
    mustache
    str
    xapi-datamodel
  )
)

(rule
  (targets LICENSE)
  (deps
    ../LICENSE
  )
  (action (copy %{deps} %{targets}))
)

(rule
  (alias generate)
  (deps
    LICENSE
    (:x main.exe)
    (source_tree templates)
  )
  (action (run %{x}))
)

