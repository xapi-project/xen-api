(library
 ((name internals)
  (public_name rpclib.internals)
  (modules (internals))
  (wrapped false)
 ))

(library
 ((name rpclib_core)
  (public_name rpclib.core)
  (modules (rpc idl rpcmarshal pythongen codegen rpc_genfake))
  (libraries (result
              rresult))
  (wrapped false)
 ))

